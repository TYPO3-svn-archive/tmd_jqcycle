<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
    <meta type="array">
        <langChildren type="integer">1</langChildren>
        <langDisable type="integer">1</langDisable>
    </meta>

  <sheets>
  
<s_image>
	<ROOT>
		<TCEforms>
			<sheetTitle>LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_sheet_content</sheetTitle>
		</TCEforms>
			
		<type>array</type>
		<el>



		<mode>
			<TCEforms>
			<label>Modus</label>
			<config>
				<type>select</type>
				<items>
					<numIndex index="0">
						<numIndex index="0">LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_mode1</numIndex>
						<numIndex index="1">IMAGE</numIndex>
					</numIndex>
					
					<numIndex index="1">
						<numIndex index="0">LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_mode2</numIndex>
						<numIndex index="1">RECORD</numIndex>
					</numIndex>

				</items>
			</config>
		</TCEforms>
		</mode>

		<images>
			<TCEforms>
				<displayCond>FIELD:mode:IN:IMAGE</displayCond>
				<label>LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_modeImage</label>
				<config>
					<type>group</type>
					<internal_type>file</internal_type>
					<allowed>png,jpeg,jpg</allowed>
					<uploadfolder>uploads/tx_tmdjqcycle</uploadfolder>
					<show_thumbs>1</show_thumbs>
					<maxitems>15</maxitems>
				</config>
			</TCEforms>
		</images>

		<content>
		<TCEforms>
			<displayCond>FIELD:mode:IN:RECORD</displayCond>
			<exclude>1</exclude>
			<label>LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_modeRecord</label>
			<config>
				<type>group</type>
				<internal_type>db</internal_type>
				<allowed>tt_content</allowed>
				<size>10</size>
				<maxitems>100</maxitems>
				<minitems>0</minitems>
				<show_thumbs>0</show_thumbs>
			</config>
		</TCEforms>
		</content>



		</el>
        
	</ROOT>
</s_image>
  
<s_configuration>
	<ROOT>
		<TCEforms>
		        <sheetTitle>LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_sheet_configuration</sheetTitle>
		</TCEforms>

        <type>array</type>
            <el>
            
<!-- fx:           'fade', // name of transition effect (or comma separated names, ex: fade,scrollUp,shuffle) -->
			<feature>
				<TCEforms>
					<label>LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_feature</label>
					<config>
						<type>select</type>
						<size>1</size>
						<items>


							<numIndex index="2" type="array">
								<numIndex index="0">blindX</numIndex>
								<numIndex index="1">blindX</numIndex>
							</numIndex>
							<numIndex index="3" type="array">
								<numIndex index="0">blindY</numIndex>
								<numIndex index="1">blindY</numIndex>
							</numIndex>
							<numIndex index="4" type="array">
								<numIndex index="0">blindZ</numIndex>
								<numIndex index="1">blindZ</numIndex>
							</numIndex>
							<numIndex index="5" type="array">
								<numIndex index="0">cover</numIndex>
								<numIndex index="1">cover</numIndex>
							</numIndex>
							<numIndex index="6" type="array">
								<numIndex index="0">curtainX</numIndex>
								<numIndex index="1">curtainX</numIndex>
							</numIndex>
							<numIndex index="7" type="array">
								<numIndex index="0">curtainY</numIndex>
								<numIndex index="1">curtainY</numIndex>
							</numIndex>
							<numIndex index="8" type="array">
								<numIndex index="0">fade</numIndex>
								<numIndex index="1">fade</numIndex>
							</numIndex>
							<numIndex index="9" type="array">
								<numIndex index="0">fadeZoom</numIndex>
								<numIndex index="1">fadeZoom</numIndex>
							</numIndex>
							<numIndex index="10" type="array">
								<numIndex index="0">growX</numIndex>
								<numIndex index="1">growX</numIndex>
							</numIndex>
							<numIndex index="11" type="array">
								<numIndex index="0">growY</numIndex>
								<numIndex index="1">growY</numIndex>
							</numIndex>
							<numIndex index="12" type="array">
								<numIndex index="0">scrollUp</numIndex>
								<numIndex index="1">scrollUp</numIndex>
							</numIndex>
							<numIndex index="13" type="array">
								<numIndex index="0">scrollDown</numIndex>
								<numIndex index="1">scrollRight</numIndex>
							</numIndex>
							<numIndex index="14" type="array">
								<numIndex index="0">scrollLeft</numIndex>
								<numIndex index="1">scrollLeft</numIndex>
							</numIndex>
							<numIndex index="15" type="array">
								<numIndex index="0">scrollRight</numIndex>
								<numIndex index="1">scrollRight</numIndex>
							</numIndex>
							<numIndex index="16" type="array">
								<numIndex index="0">scrollHorz</numIndex>
								<numIndex index="1">scrollHorz</numIndex>
							</numIndex>
							<numIndex index="17" type="array">
								<numIndex index="0">scrollVert</numIndex>
								<numIndex index="1">scrollVert</numIndex>
							</numIndex>
							<numIndex index="18" type="array">
								<numIndex index="0">shuffle</numIndex>
								<numIndex index="1">shuffle</numIndex>
							</numIndex>
							<numIndex index="19" type="array">
								<numIndex index="0">slideX</numIndex>
								<numIndex index="1">slideX</numIndex>
							</numIndex>
							<numIndex index="20" type="array">
								<numIndex index="0">slideY</numIndex>
								<numIndex index="1">slideY</numIndex>
							</numIndex>
							<numIndex index="21" type="array">
								<numIndex index="0">toss</numIndex>
								<numIndex index="1">toss</numIndex>
							</numIndex>
							<numIndex index="22" type="array">
								<numIndex index="0">turnUp</numIndex>
								<numIndex index="1">turnUp</numIndex>
							</numIndex>
							<numIndex index="23" type="array">
								<numIndex index="0">turnDown</numIndex>
								<numIndex index="1">turnDown</numIndex>
							</numIndex>
							<numIndex index="24" type="array">
								<numIndex index="0">turnLeft</numIndex>
								<numIndex index="1">turnLeft</numIndex>
							</numIndex>
							<numIndex index="25" type="array">
								<numIndex index="0">turnRight</numIndex>
								<numIndex index="1">turnRight</numIndex>
							</numIndex>
							<numIndex index="26" type="array">
								<numIndex index="0">uncover</numIndex>
								<numIndex index="1">uncover</numIndex>
							</numIndex>
							<numIndex index="27" type="array">
								<numIndex index="0">wipe</numIndex>
								<numIndex index="1">wipe27</numIndex>
							</numIndex>
							<numIndex index="28" type="array">
								<numIndex index="0">zoom</numIndex>
								<numIndex index="1">zoom</numIndex>
							</numIndex>
						</items>
					</config>
				</TCEforms>
			</feature>












<!-- 
    timeoutFn:     null,  // callback for determining per-slide timeout value:  function(currSlideElement, nextSlideElement, options, forwardFlag)
    before:        null,  // transition callback (scope set to element to be shown):     function(currSlideElement, nextSlideElement, options, forwardFlag) 
    after:         null,  // transition callback (scope set to element that was shown):  function(currSlideElement, nextSlideElement, options, forwardFlag) 
    cleartype:     !$.support.opacity,  // true if clearType corrections should be applied (for IE)
    next:          null,  // selector for element to use as click trigger for next slide 
    prev:          null,  // selector for element to use as click trigger for previous slide 
    prevNextClick: null,  // callback fn for prev/next clicks:  function(isNext, zeroBasedSlideIndex, slideElement) 
    pager:         null,  // selector for element to use as pager container 
    pagerClick:    null,  // callback fn for pager clicks:  function(zeroBasedSlideIndex, slideElement) 
    pagerEvent:   'click', // name of event which drives the pager navigation 
    pagerAnchorBuilder: null, // callback fn for building anchor links:  function(index, DOMelement) 
    end:           null,  // callback invoked when the slideshow terminates (use with autostop or nowrap options): function(options) 
    easing:        null,  // easing method for both in and out transitions 
    easeIn:        null,  // easing for "in" transition 
    easeOut:       null,  // easing for "out" transition 
    shuffle:       null,  // coords for shuffle animation, ex: { top:15, left: 200 } 
    animIn:        null,  // properties that define how the slide animates in 
    animOut:       null,  // properties that define how the slide animates out 
    cssBefore:     null,  // properties that define the initial state of the slide before transitioning in 
    cssAfter:      null,  // properties that defined the state of the slide after transitioning out 
    fxFn:          null,  // function used to control the transition: function(currSlideElement, nextSlideElement, options, afterCalback, forwardFlag) 
    startingSlide: 0,     // zero-based index of the first slide to be displayed 
    fit:           0,     // force slides to fit container 
    containerResize: 1,   // resize container to fit largest slide 
    pauseOnPagerHover: 0, // true to pause when hovering over pager link 
    autostop:      0,     // true to end slideshow after X transitions (where X == slide count) 
    autostopCount: 0,     // number of transitions (optionally used with autostop to define X) 
    slideExpr:     null,  // expression for selecting slides (if something other than all children is required) 
    cleartypeNoBg: false, // set to true to disable extra cleartype fixing (leave false to force background color setting on slides) 
    nowrap:        0,     // true to prevent slideshow from wrapping 
    fastOnEvent:   0,     // force fast transitions when triggered manually (via pager or prev/next); value == time in ms 
    randomizeEffects: 1,  // valid when multiple effects are used; true to make the effect sequence random 
    rev:           0,     // causes animations to transition in reverse 
    manualTrump:   true,  // causes manual transition to stop an active transition instead of being ignored 
    requeueOnImageNotLoaded: true, // requeue the slideshow if any image slides are not yet loaded 
    requeueTimeout: 250   // ms delay for requeue
-->


			<width>
				<TCEforms>
					<label>LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_width</label>
					<config>
						<type>input</type>
						<size>10</size>
						<value>1500</value>
					</config>
				</TCEforms>
			</width>

			<height>
				<TCEforms>
					<label>LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_height</label>
					<config>
						<type>input</type>
						<size>10</size>
						<default>1500</default>
					</config>
				</TCEforms>
			</height>

<!-- timeout:       4000,  // milliseconds between slide transitions (0 to disable auto advance) -->
			<timeout>
				<TCEforms>
					<label>LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_timeout</label>
					<config>
						<type>input</type>
						<size>10</size>
						<default>4000</default>
					</config>
				</TCEforms>
			</timeout>


<!--     speed:         1000,  // speed of the transition (any valid fx speed value) -->			
			<speed>
				<TCEforms>
					<label>LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_speed</label>
					<config>
						<type>input</type>
						<size>10</size>
						<default>1000</default>
					</config>
				</TCEforms>
			</speed>
<!--   speedIn:       null,  // speed of the 'in' transition -->
			<speedIn>
				<TCEforms>
					<label>LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_speedIn</label>
					<config>
						<type>input</type>
						<size>10</size>
						<default>0</default>
					</config>
				</TCEforms>
			</speedIn>
 
<!--  speedOut:      null,  // speed of the 'out' transition -->
			<speedOut>
				<TCEforms>
					<label>LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_speedOut</label>
					<config>
						<type>input</type>
						<size>10</size>
						<default>0</default>
					</config>
				</TCEforms>
			</speedOut>
 
     
<!-- random:        0,     // true for random, false for sequence (not applicable to shuffle fx)  -->
            <random>
	            <TCEforms>
	                <label>LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_random</label>
	                <config>
	                   <type>check</type>
	                   <value>0</value>
	                </config>
	            </TCEforms>
            </random>

<!--   sync:          1,     // true if in/out transitions should occur simultaneously -->  
            <sync>
	            <TCEforms>
	                <label>LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_sync</label>
	                <config>
	                   <type>check</type>
	                   <value>1</value>
	                </config>
	            </TCEforms>
            </sync>

<!-- pause:         0,     // true to enable "pause on hover" -->
            <pause>
	            <TCEforms>
	            <!-- displayCond>FIELD:feature:IN:8,18</displayCond -->
	                <label>LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_pause</label>
	                <config>
	                   <type>check</type>
	                   <value>0</value>
	                </config>
	            </TCEforms>
            </pause>

 <!-- delay:         0,     // additional delay (in ms) for first transition (hint: can be negative) -->
             <delay>
	            <TCEforms>
	                <label>LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_delay</label>
	                <config>
						<type>input</type>
						<size>10</size>
						<default>0</default>
	                </config>
	            </TCEforms>
            </delay>

 <!-- continuous:    0,     // true to start next transition immediately after current one completes -->
             <continuous>
	            <TCEforms>
	                <label>LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_continuous</label>
	                <config>
						<type>input</type>
						<size>10</size>
						<default>0</default>
	                </config>
	            </TCEforms>
            </continuous>

 

            </el>

          </ROOT>
</s_configuration>


<s_advanced>
	<ROOT>
		<TCEforms>
			<sheetTitle>LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_sheet_advanced</sheetTitle>
		</TCEforms>
			
		<type>array</type>
		<el>


           <useAdvanced>
            <TCEforms>
            <!-- displayCond>FIELD:feature:IN:8,18</displayCond -->
                <label>LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_useAdvanced</label>
                <config>
                   <type>check</type>
                   <value>0</value>
                </config>
            </TCEforms>
           </useAdvanced>


		<advanced>
			<TCEforms>
			<label>LLL:EXT:tmd_jqcycle/locallang_db.xml:tx_tmdjqcycle_advanced</label>
			<config>
				<type>text</type>
	     		 <size>15</size>
                 <rows>6</rows>
                 <default>fx:   'shuffle', 
shuffle: { 
    top:  -230, 
    left:  230 
}, 
delay: -2000</default>
			</config>
		</TCEforms>
		</advanced>

		</el>
        
	</ROOT>
</s_advanced>


		
		
		
		
		
  </sheets>

</T3DataStructure>